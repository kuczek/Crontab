{
  "name": "hexmedia/crontab",
  "type": "library",
  "description": "Managing crontab",
  "keywords": ["crontab", "crontab manager", "tasks"],
  "authors": [
    {
      "name": "Krystian Kuczek",
      "email": "krystian@hexmedia.pl",
      "role": "Developer",
      "homepage": "http://www.hexmedia.pl"
    }
  ],
  "homepage": "http://www.hexmedia.pl/crontab",
  "license": "MIT",
  "support": {
    "email": "krystian@hexmedia.pl",
    "issues": "https://github.com/Hexmedia/Crontab/issues"
  },
  "autoload": {
    "psr-4": {"Hexmedia\\Crontab\\": "./src"}
  },
  "require": {
    "php": ">=5.3.3",
    "symfony/console": ">=2.3"
  },
  "require-dev": {
    "phpunit/phpunit" : ">=4.0",
    "sebastian/phpcpd": "^2.0",
    "sebastian/phpdcd": "^1.0",
    "phpspec/phpspec": "~2.4",
    "henrikbjorn/phpspec-code-coverage": "*",
    "symfony/yaml": ">=2.3",
    "symfony/config": ">=2.3",
    "austinhyde/iniparser": "~1.0@dev",
    "zf1/zend-config" : "1.12.11",
    "zendframework/zend-config": "~2.5",
    "phpmd/phpmd": "~2.3",
    "pdepend/pdepend": "~2.1",
    "squizlabs/php_codesniffer": "^2.5",
    "phploc/phploc": ">=2.0",
    "symfony/process": ">=2.3",
    "seld/jsonlint": "^1.4",
    "hexmedia/yaml-linter": "^0.2.0",
    "sclable/xml-lint": "^0.2.1"
  },
  "suggest": {
    "symfony/yaml": "Enables support for yaml configuration files.",
    "symfony/config": "Enables support for ini configuration files.",
    "austinhyde/iniparser": "Enables support for ini configuration files.",
    "zf1/zend-config": "Enables support for ini configuration files.",
    "zendframework/zend-config": "Enables support for ini and yaml configuration files."
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
